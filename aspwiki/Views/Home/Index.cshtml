@{
    ViewData["Title"] = "Home";
}
@using ASPWiki.Model;
@model List<WikiPage>

<section>
    <div class="row">
        <div class="col-md-6">
            <h1>Latest</h1>
            @for (var i = 0; i < @Model.Count; i++)
            {
                @Html.Partial("PageLink", @Model[i]);
            }
        </div>

        <div class="col-md-6">
            <h1>Popular</h1>
            @for (var i = 0; i < @Model.Count; i++)
            {
                @Html.Partial("PageLink", Model[i]);
            }
        </div>
    </div>
</section>