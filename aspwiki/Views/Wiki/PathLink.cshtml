@model string[]

@{
    string pathUrl = "/Wiki/View";
    for (int i = 0; i < @Model.Length; i++)
    {
        pathUrl += "/" + @Model[i];
        <text>
            @if (i != Model.Length - 1)
            {
                <a href="@pathUrl">@Model[i]</a>
                <i class="fa fa-angle-double-right" aria-hidden="true"></i>
            }
            else
            {
                @Model[i]
            }
        </text>
    }
}
